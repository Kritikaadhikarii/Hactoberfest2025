{
    "manifest_version": 3,
    "name": "API Tester",
    "version": "1.0",
    "description": "A simple tool to test API endpoints directly from the browser.",
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon48.png"
    },
    "permissions": ["activeTab"]
  }
  
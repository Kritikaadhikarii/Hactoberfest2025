    # Add two numbers
    num1 = 10
    num2 = 20
    sum_result = num1 + num2
    print(f"The sum is: {sum_result}")

    # Check if a number is even or odd
    number = int(input("Enter a number: "))
    if number % 2 == 0:
        print("The number is Even")
    else:
        print("The number is Odd")
